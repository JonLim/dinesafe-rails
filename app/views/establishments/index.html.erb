<h1>List of Establishments</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @establishments.each do | establishment | %>
      <tr>
        <td><%= establishment.name %></td>
        <td><%= establishment.address %></td>
        <td><%= link_to 'Show', establishment %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p><%= paginate @establishments %></p>

<br />
<br />
<br />
<br />


<%= form_tag({action: :upload}, multipart: true) do %>
  <%= file_field_tag 'dinesafe_dump' %>
  <%= submit_tag("Upload") %>
<% end %>

